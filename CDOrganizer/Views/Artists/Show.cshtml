@using CdOrganizer.Models;
<h3>Here are all the items in this category:</h3>

<ol>
@foreach (var oneCD in @Model["cds"])
{
  <li><a href='/artists/@Model["artist"].GetId()/cds/@oneCD.GetId()'>@oneCD.GetTitle()</a></li>
}
</ol>

<p><a href='/artists/@Model["artist"].GetId()/cds/new'>Add a new CD</a></p>
<p><a href='/artists'>Return to artist list</a></p>
